    <file src="..\..\_build\x64\Release\VGLL\bin\VGLL\VGLL.winmd" target="lib\netcoreapp3.0\VGLL.winmd" />
    <file src="..\..\_build\x64\Release\VGLL\bin\VGLL\VGLL.winmd" target="lib\uap10.0\VGLL.winmd" />
    <file src="..\..\_build\x64\Release\VGLL\bin\VGLL\VGLL.winmd" target="lib\net46\VGLL.winmd" />
    <file src="..\..\_build\x64\Release\VGLLProjection\bin\VGLLProjection.dll" target="lib\net6.0-windows10.0.19041.0\VGLLProjection.dll" />
    <!--Architecture-specific implementation DLLs should be copied into RID-relative folders-->
    <file src="..\..\_build\x64\Release\VGLL\bin\VGLL\VGLL.dll" target="runtimes\win10-x64\native\VGLL.dll" />
  </files>
</package>
